{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "JwtConfig": {
        "Secret": "[JWT_SECRET]",
        "ExpiryTimeFrame": "00:01:00"
    },
  "ConnectionStrings": {
    "DB": "Server=[SERVER];uid=[USERNAME];pwd=[PASSWORD];Database=[DATABASE_NAME];TrustServerCertificate=True"
  },
    "EmailConfig": {
        "API_KEY": "[API_KEY]"
    },
    "MailSettings": {
        "DefaultSender": "[EMAIL_ADDRESS]",
        "Password": "[EMAIL_PASSWORD]",
        "DisplayName": "AspNetCore.WebApi",
        "Provider": "smtp.gmail.com",
        "Port": 587
    },
    "ApplicationUrl": "http://localhost:5050/"
}
